    "keywords": ["zlib", "gzip", "gz", "deflate", "inflate", "compression", "decompression", "RFC 1950", "RFC 1951", "RFC 1952", "ReactPHP"],
    "homepage": "https://github.com/clue/reactphp-zlib",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian LÃ¼ck",
            "email": "christian@clue.engineering"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-zlib": "*",
        "react/stream": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^6.5",
        "react/event-loop": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Clue\\React\\Zlib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clue\\Tests\\React\\Zlib\\": "tests/"
        }
